<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th></th>
      <th>Title</th>
      <th>Opinion</th>
      <th>Category</th>
      <th>Favorite</th>
      <th>Comment</th>
      <th>Created date</th>
      <th colspan="6"></th>
    </tr>
  </thead>
  <tbody>
    <% current_user.@blogs.each do |blog| %>
      <tr>
        <td>
        </td>
        <td><%= link_to blog.user.name, user_blog_path(blog.user_id) %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>